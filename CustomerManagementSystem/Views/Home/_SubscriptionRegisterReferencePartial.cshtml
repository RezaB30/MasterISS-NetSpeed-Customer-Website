@model CMS.ViewModels.Home.SubcriptionRegisterViewModel
@using CMS.Localization
@{
    ViewBag.CommitmentLengthList = ViewBag.CommitmentLengthList ?? new SelectList(Enumerable.Empty<SelectListItem>());
}
<div class="pb-5" data-wizard-type="step-content">
    <div class="separator separator-dashed my-3"></div>
    <div class="pb-10 pb-lg-5 text-center">
        <hr class="my-4">
    </div>
    <div class="row">
        <!--end::Form Group-->
        <!--begin::Form Group-->
        <div class="col-lg-12 col-md-12">
            <div class="form-group">
                <label class="font-size-h6 font-weight-bolder m-2 text-dark">@Html.DisplayNameFor(m => m.ReferralDiscount.ReferenceNo)</label>
                @Html.EditorFor(m => m.ReferralDiscount.ReferenceNo, new { htmlAttributes = new { @class = "form-control h-auto p-3 border-0 rounded-lg font-size-h6", @placeholder = Common.Optional } })
                <div class="fv-plugins-message-container">
                    @Html.ValidationMessageFor(m => m.ReferralDiscount.ReferenceNo, null, new { @class = "fv-help-block" , @autocomplete = "off" })
                </div>
            </div>
        </div>
    </div>
</div>