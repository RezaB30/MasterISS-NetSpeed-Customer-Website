@model bool?

@if (Model == true)
{
    <img class="boolean-symbol" src="@Url.Content("~/Content/img/check.svg")" style="width:1.7em !important;" alt="V" />
}
else if (Model == false)
{
    <img class="boolean-symbol" src="@Url.Content("~/Content/img/crossed.svg")" style="width:1.7em !important;" alt="X" />
}
else
{
    @:-
}
